{
    "name": "renoki-co/horizon-exporter",
    "description": "Export Laravel Horizon metrics using this Prometheus exporter.",
    "keywords": ["laravel", "php", "horizon", "exporter", "prometheus", "grafana", "queues", "queue", "job"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/horizon-exporter",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "promphp/prometheus_client_php": "^2.0",
        "illuminate/config": "^7.40|^8.40",
        "illuminate/routing": "^7.40|^8.40",
        "illuminate/support": "^7.40|^8.40",
        "laravel/horizon": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\HorizonExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\HorizonExporter\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.21.1",
        "predis/predis": "^1.1"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\HorizonExporter\\HorizonExporterServiceProvider"
            ]
        }
    }
}
